# A sample of ~/.w3m/keymap (lynx-like)
#
# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D

keymap	C-@	MARK
keymap	C-a	BEGIN
keymap	C-b	BACK
keymap	C-e	END
keymap	C-f	GOTO_LINK
keymap	C-g	LINE_INFO
keymap	C-h	HISTORY
keymap	TAB	NEXT_LINK
keymap	C-j	GOTO_LINK
keymap	C-k	COOKIE
keymap	C-l	REDRAW
keymap	C-m	GOTO_LINK
keymap	C-n	NEXT_LINK
keymap	C-p	PREV_LINK
keymap	C-q	CLOSE_TAB
keymap	C-r	RELOAD
keymap	C-s	SEARCH
keymap	C-t	TAB_LINK
keymap	C-u	PREV_LINK
keymap	C-v	NEXT_PAGE
keymap	C-w	REDRAW
keymap	C-z	SUSPEND

keymap	SPC	NEXT_PAGE
keymap	!	SHELL
keymap	\"	REG_MARK

        <array>
            <string>%PREFIX%/bin</string>
            <string>%APPLICATIONS_DIR%/KDE4</string>
        </array>
    <key>ThrottleInterval</key>
        <integer>60</integer>
    <key>Disabled</key>
        <true/>

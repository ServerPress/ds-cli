diff -ruN ./d1mach.f ../src.patched/d1mach.f
--- ./d1mach.f	1993-02-18 18:06:52.000000000 +0100
+++ ../src.patched/d1mach.f	1999-10-06 23:13:26.000000000 +0200
@@ -69,20 +69,14 @@
 C   930201  Added DEC Alpha and SGI constants.  (RWC and WRB)
 C***END PROLOGUE  D1MACH
 C
-      INTEGER SMALL(4)
-      INTEGER LARGE(4)
-      INTEGER RIGHT(4)
-      INTEGER DIVER(4)
-      INTEGER LOG10(4)
-C
-      DOUBLE PRECISION DMACH(5)
-      SAVE DMACH
-C
-      EQUIVALENCE (DMACH(1),SMALL(1))
-      EQUIVALENCE (DMACH(2),LARGE(1))
-      EQUIVALENCE (DMACH(3),RIGHT(1))
-      EQUIVALENCE (DMACH(4),DIVER(1))
-      EQUIVALENCE (DMACH(5),LOG10(1))
+C
+	external dlamch
+      DOUBLE PRECISION DMACH(5),dlamch
+	integer iflag
+
+      SAVE DMACH,iflag
+	data iflag/0/
+C
 C
 C     MACHINE CONSTANTS FOR THE AMIGA
 C     ABSOFT FORTRAN COMPILER USING THE 68020/68881 COMPILER OPTION
@@ -496,6 +490,15 @@
       IF (I .LT. 1 .OR. I .GT. 5) CALL XERMSG ('SLATEC', 'D1MACH',
      +   'I OUT OF BOUNDS', 1, 2)
 C
+	if (iflag.eq.0) then
+	   iflag=1
+	   dmach(1)=dlamch('u')
+	   dmach(2)=dlamch('o')
+	   dmach(3)=dlamch('e')
+	   dmach(4)=dlamch('p')
+	   dmach(5)=dlamch('b')
+	   dmach(5)=log10(dmach(5))
+	endif
       D1MACH = DMACH(I)
       RETURN
 C

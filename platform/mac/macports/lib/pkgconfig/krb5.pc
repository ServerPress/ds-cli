prefix=/opt/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
vendor=MIT

defccname=KCM:
defktname=FILE:/etc/krb5.keytab
defcktname=FILE:/opt/local/var/krb5/user/%{euid}/client.keytab


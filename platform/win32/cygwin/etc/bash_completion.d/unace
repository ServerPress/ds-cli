# unace(1) completion

have unace &&
_unace()
{
    local cur

    COMPREPLY=()
    _get_comp_words_by_ref cur

    if [[ "$cur" == -* ]] ; then
        COMPREPLY=( $( compgen -W '-c -c- -f -f- -o -o- -p -y -y-' -- "$cur" ) )
    else
        if [ $COMP_CWORD -eq 1 ]; then
            COMPREPLY=( $( compgen -W 'e l t v x' -- "$cur" ) )
        else
            _filedir ace
        fi
    fi
} &&
complete -F _unace unace

